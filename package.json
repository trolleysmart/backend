{
  "name": "smart-grocery-backend",
  "version": "0.1.0",
  "description": "Smart Grocery Backend",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "start:watch": "nodemon --exec npm start",
    "build": "babel src --out-dir dist && npm run generate-graphql-schema",
    "generate-graphql-schema": "babel-node scripts/generate-graphql-schema.js",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test-verbose": "jest --verbose",
    "test-verbose:watch": "npm run test-verbose -- --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/microbusiness/smart-grocery-backend.git"
  },
  "author": "Micro Business ltd<micro.business.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/microbusiness/smart-grocery-backend#readme",
  "dependencies": {
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.6",
    "graphql-fields": "^1.0.2",
    "graphql-relay": "^0.5.1",
    "immutable": "^3.8.1",
    "micro-business-parse-server-backend": "^0.1.21",
    "micro-business-parse-server-common": "^0.1.50",
    "smart-grocery-parse-server-common": "^0.1.93",
    "store-crawler": "^0.1.85"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "flow-bin": "^0.46.0",
    "jest": "^20.0.0",
    "nodemon": "^1.11.0",
    "uuid": "^3.0.1"
  },
  "jest": {
    "rootDir": "./src"
  }
}
